#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/dev/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/dev/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/dev/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/@react-router+dev@7.10.1_@r_1ec244e67b91535d336d8c48f3196ea0/node_modules/@react-router/dev/bin.js" "$@"
fi
