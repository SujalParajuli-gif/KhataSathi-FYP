#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/serve/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/serve/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules:/mnt/d/KhataSathi-FYP/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/serve/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../@react-router+serve@7.10.1__f8b0300a5c4db6872a014b633950a65a/node_modules/@react-router/serve/bin.js" "$@"
fi
